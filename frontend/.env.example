# Environment variables for the frontend application

# Google Maps API Key
NEXT_PUBLIC_GOOGLE_MAPS_API_KEY=your_google_maps_api_key_here

# Backend API URL (dengan /api suffix)
NEXT_PUBLIC_API_URL=https://purrpal-backend-XXXXX-et.a.run.app/api

# Supabase untuk frontend auth
NEXT_PUBLIC_SUPABASE_URL=your_existing_supabase_url
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_existing_supabase_anon_key